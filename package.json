{
  "name": "refracter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-contextmenu": "^2.3.1",
    "react-dom": "15.4.2",
    "react-form": "^1.2.6",
    "react-input-range": "^1.0.2",
    "react-router": "^3.0.2",
    "react-toastify": "^1.2.1",
    "react-youtube": "^7.3.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.4"
  },
  "scripts": {
    "build-css": "node-sass src/index.scss -o src/",
    "watch-css": "npm run build-css && node-sass src/index.scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
